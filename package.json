{
  "name": "casey-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Casey Gollan's public information",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc && esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js",
    "start": "wrangler dev",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cloudflare",
    "workers"
  ],
  "author": "Casey Gollan",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@cloudflare/workers-types": "^4.20241127.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.86.0"
  }
}
